<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#222" >

<LinearLayout
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="40dp"
    android:layout_gravity="center"
    android:layout_marginTop="1dp" >
   
    <LinearLayout android:orientation="horizontal" android:layout_marginTop="12dp" android:layout_marginBottom="8dp" android:layout_width="240.0px" android:layout_height="79.0px">
        
    <RelativeLayout android:layout_width="80.0px" android:layout_height="79.0px" android:layout_marginRight="20dp" >
          <nemesis.hybrid.engine.Statbg android:layout_width="80.0px" android:layout_height="79.0px" />
          <Button android:id="@+id/stat" android:layout_gravity="center_horizontal" android:background="#00000000" android:layout_width="80.0px" android:layout_height="73.0px" android:layout_marginTop="3.0px" />
          <TextView android:textSize="12.0dip" android:textColor="#ffcccccc" android:gravity="center" android:background="#80000000" android:layout_width="80.0px" android:layout_height="wrap_content" android:text="STATUS BAR" android:singleLine="true" android:layout_alignParentBottom="true" />
   </RelativeLayout>
   
   <RelativeLayout android:layout_width="80.0px" android:layout_height="79.0px" >
          <nemesis.hybrid.engine.Clockbg android:layout_width="80.0px" android:layout_height="79.0px" />
          <Button android:id="@+id/clock" android:layout_gravity="center_horizontal" android:background="#00000000" android:layout_width="80.0px" android:layout_height="73.0px" android:layout_marginTop="3.0px" />
          <TextView android:textSize="12.0dip" android:textColor="#ffcccccc" android:gravity="center" android:background="#80000000" android:layout_width="80.0px" android:layout_height="wrap_content" android:text="CLOCK BG" android:singleLine="true" android:layout_alignParentBottom="true" />
   </RelativeLayout>
   

   </LinearLayout>
   
        <LinearLayout android:orientation="horizontal" android:layout_marginBottom="8dp" android:layout_width="240.0px" android:layout_height="79.0px">
        
    <RelativeLayout android:layout_width="80.0px" android:layout_height="79.0px" android:layout_marginRight="20dp">
          <nemesis.hybrid.engine.Panelbg android:layout_width="80.0px" android:layout_height="79.0px" />
          <Button android:id="@+id/panel" android:layout_gravity="center_horizontal" android:background="#00000000" android:layout_width="80.0px" android:layout_height="73.0px" android:layout_marginTop="3.0px" />
          <TextView android:textSize="12.0dip" android:textColor="#ffcccccc" android:gravity="center" android:background="#80000000" android:layout_width="80.0px" android:layout_height="wrap_content" android:text="PANEL COLOR" android:singleLine="true" android:layout_alignParentBottom="true" />
   </RelativeLayout>
   
   <RelativeLayout android:layout_width="80.0px" android:layout_height="79.0px" >
          <nemesis.hybrid.engine.Tilebg android:layout_width="80.0px" android:layout_height="79.0px" />
          <Button android:id="@+id/tile" android:layout_gravity="center_horizontal" android:background="#00000000" android:layout_width="80.0px" android:layout_height="73.0px" android:layout_marginTop="3.0px" />
          <TextView android:textSize="12.0dip" android:textColor="#ffcccccc" android:gravity="center" android:background="#80000000" android:layout_width="80.0px" android:layout_height="wrap_content" android:text="TILE COLOR" android:singleLine="true" android:layout_alignParentBottom="true" />
   </RelativeLayout>

   
   </LinearLayout>
    <LinearLayout android:orientation="horizontal" android:layout_marginBottom="8dp" android:layout_width="240.0px" android:layout_height="79.0px">
        
   <RelativeLayout android:layout_width="80.0px" android:layout_height="79.0px" android:layout_marginRight="20dp">
          <nemesis.hybrid.engine.Tiletext android:layout_width="80.0px" android:layout_height="79.0px" />
          <Button android:id="@+id/tiletext" android:layout_gravity="center_horizontal" android:background="#00000000" android:layout_width="80.0px" android:layout_height="73.0px" android:layout_marginTop="3.0px" />
          <TextView android:textSize="12.0dip" android:textColor="#ffcccccc" android:gravity="center" android:background="#80000000" android:layout_width="80.0px" android:layout_height="wrap_content" android:text="TILE TEXT" android:singleLine="true" android:layout_alignParentBottom="true" />
   </RelativeLayout>
   
   <RelativeLayout android:layout_width="80.0px" android:layout_height="79.0px" >
          <Button android:id="@+id/clocktext" android:layout_gravity="center_horizontal" android:background="#00000000" android:layout_width="80.0px" android:layout_height="73.0px" android:layout_marginTop="3.0px" />
          <nemesis.hybrid.engine.Clocktextbg android:layout_width="80.0px" android:layout_height="79.0px" />
          <TextView android:textSize="12.0dip" android:textColor="#ffcccccc" android:gravity="center" android:background="#80000000" android:layout_width="80.0px" android:layout_height="wrap_content" android:text="CLOCK TEXT" android:singleLine="true" android:layout_alignParentBottom="true" />
   </RelativeLayout>

     </LinearLayout>
     
    </LinearLayout>
    
  </RelativeLayout>